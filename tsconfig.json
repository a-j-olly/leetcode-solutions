{
  "compilerOptions": {
    "target": "esnext",
    "strict": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "module":"ESNext",
    "moduleResolution":"node",
    "esModuleInterop": true, 
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true, 
    "isolatedModules": true, 
    "resolveJsonModule": true,
    "outDir": "dist"
  },
  "exclude": ["node_modules", "__tests__/*.test.*", "dist"]
}